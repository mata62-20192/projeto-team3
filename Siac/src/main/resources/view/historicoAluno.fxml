<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>
<?import java.net.URL?>
<AnchorPane styleClass="student-section" xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/8.0.151"
            fx:controller="br.ufba.team3.siac.controller.HistoricoController">
    <children>
        <VBox>
            <children>
                <Label text="Impressão Histórico do Aluno">
                    <font>
                        <Font name="System Bold" size="29.0"/>
                    </font>
                </Label>
                <HBox>
                    <children>
                        <Label text="Curso*:"/>
                        <ListView minWidth="600" maxHeight="300" fx:id="minhaListViewAlunos"/>
                    </children>
                </HBox>
                <HBox>
                    <children>
                        <Button fx:id="imprimir" onAction="#imprimir" mnemonicParsing="false" text="Salvar Curriculo"/>
                    </children>
                </HBox>
                <Label fx:id="successError" text=""/>
            </children>
        </VBox>
    </children>
    <stylesheets>
        <URL value="@demo.css"/>
    </stylesheets>
</AnchorPane>
